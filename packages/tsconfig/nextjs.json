{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Next.js TypeScript configuration",
	"extends": "./base.json",
	"compilerOptions": {
		"lib": ["DOM", "DOM.Iterable", "ES2022"],
		"moduleResolution": "Bundler",
		"jsx": "preserve"
	},
	"include": [
		".eslintrc.cjs",
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		"**/*.cjs",
		"**/*.js",
		"src",
		"next-env.d.ts",
		".next/types/**/*.ts"
	],
	"exclude": ["node_modules"]
}
